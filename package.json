{
  "name": "kakao-clone",
  "version": "1.0.0",
  "description": "Node.js 를 이용한 Kakao RestFul Server",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build public/bundle.js",
    "dev": "node_modules/.bin/babel server/src --watch --out-dir server/build && supervisor server/build/server.js"
    "build": "babel --presets @babel/preset-env server/src --watch --out-dir server/build",
    "start": "node server/build/server.js"
  },
  "author": "sungmun",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "express": "^4.16.4",
    "next": "^7.0.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "express-session": "^1.15.6",
    "mongoose": "^5.3.11",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sungmun/Kakao-Clone-Server.git"
  },
  "bugs": {
    "url": "https://github.com/sungmun/Kakao-Clone-Server/issues"
  },
  "homepage": "https://github.com/sungmun/Kakao-Clone-Server#readme"
}
